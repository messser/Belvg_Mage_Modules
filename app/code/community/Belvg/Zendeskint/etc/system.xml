<config>
    <sections>
        <zendeskint translate="label">
            <label>Zendesk Integration</label>
            <tab>belvg</tab>
            <frontend_type>text</frontend_type>
            <sort_order>15</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <info translate="label comment">
                    <label>Info</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <comment><![CDATA[<div style="padding:10px;background-color:#fff;border:1px solid #ddd;margin-bottom:7px;"><div style="float:left;margin-right:10px;background:url('data:image/png;base64,') no-repeat; width: 54px; height: 54px;"></div>This module was developed by <a href="http://www.belvg.com" target="_blank">www.BelVG.com</a>.<br>For docs & examples refer to the BelVG <a href="http://www.belvg.com" target="_blank">web site</a>.<br/>We have the largest selections of  <a href="http://store.belvg.com/extensions.html" target="_blank">extensions</a> and <a href="http://store.belvg.com/themes.html" target="_blank">themes</a> on Magento.</div>]]></comment>
                    <fields>
                        <info translate="label">
                            <frontend_type>text</frontend_type>
                            <frontend_model>zendeskint/adminhtml_config_info</frontend_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </info>
                    </fields>
                </info>
                <settings translate="label">
                    <label>General Settings</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>20</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <domain translate="label tooltip">
                            <label>Zendesk Domain</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <validate>required-entry validate-url</validate>
                        </domain>
                        <agent translate="label tooltip">
                            <label>Agent Email Address</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <validate>required-entry validate-email</validate>
                        </agent>
                        <token translate="label tooltip">
                            <label>Agent Token</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <validate>required-entry</validate>
                        </token>
                        <auth_token translate="label tooltip">
                            <label>Remote Authentication Token</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <validate>required-entry</validate>
                            <comment><![CDATA[<style>#zendeskint_settings_token{font-size: 11px;}#zendeskint_settings_auth_token{font-size:9px;}</style>]]></comment>
                        </auth_token>
                    </fields>
                </settings>
            </groups>
        </zendeskint>
    </sections>
</config>